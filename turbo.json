{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {
      "cache": true,
      "outputs": []
    },
    "typecheck": {
      "cache": true,
      "outputs": []
    },
    "test": {
      "cache": false,
      "outputs": ["coverage/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["apps/web/.next/**", "apps/web/public/wasm/**"]
    },
    "dev": {
      "cache": false
    },
    "e2e": {
      "cache": false
    }
  }
}
