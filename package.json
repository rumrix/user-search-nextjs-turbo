{
  "name": "user-search-nextjs-turbo",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "wasm:build": "pnpm --filter @user-search/avatar-wasm build",
    "predev": "pnpm run wasm:build",
    "dev": "turbo dev --filter=web",
    "dev:checked": "pnpm run verify && pnpm run dev",
    "dev:mock": "pnpm --filter web dev:mock",
    "prestart": "pnpm run wasm:build",
    "build": "turbo build",
    "start": "turbo start --filter=web",
    "prebuild": "pnpm run wasm:build",
    "start:checked": "pnpm run verify && pnpm run start",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "verify": "pnpm run lint && pnpm run typecheck && pnpm run test",
    "e2e": "turbo e2e --filter=web"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cypress": "^13.7.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  }
}
